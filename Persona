public class Persona {
    private int nombre;
    private String apellido;
    private String correo;
    private String rut;
    private String estrellitas;
    private String comentarios;
    public Persona(int nombre, String apellido, String correo, String rut, String estrellitas, String comentarios){
        this.nombre = nombre;
        this.apellido = apellido;
        this.correo = correo;
        this.rut = rut;
        this.estrellitas = estrellitas;
        this. comentarios = comentarios;
    }

    @Override
    public String toString() {
        return "persona{" +
                "nombre='" + nombre + '\'' +
                ", apellido='" + apellido + '\'' +
                ", correo='" + correo + '\'' +
                ", rut='" + rut + '\'' +
                ", estrellitas='" + estrellitas + '\'' +
                ", comentarios='" + comentarios + '\'' +
                '}';
    }
    public void  getNombre(){
        System.out.println(nombre);
    }
}
